{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "plugins": [
      {
        "name": "typescript-tslint-plugin"
      }
    ],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "isolatedModules": false,
    "noEmit": true,
    "strictNullChecks": false
  },
  "include": [
    "src", "temp/reloadable.ts", "temp/resourceManager.ts", "temp/resource.ts", "temp/old/startupResource.ts", "temp/urid.ts", "temp/old"
  ]
}
